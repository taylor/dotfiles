aterm.font4: -adobe-courier-medium-r-normal--25-180-100-100-m-150-iso8859-1
*VT100*translations:    #override \n\
	<Btn4Down>,<Btn4Up>: string("") \n\
	<Btn5Down>,<Btn5Up>: string("") \n\
	Ctrl<Key>=: larger-vt-font() \n\
	Ctrl<Key>-: smaller-vt-font()

XTerm*metaSendsEscape:	True


! ! enable colors in native X applications
#ifdef COLOR
 *customization                : -color
#endif

Xcursor.theme                 : Human

URxvt.urgentOnBell: true
XTerm.urgentOnBell: true

rxvt*rightScrollBar: True
rxvt*background:       black
